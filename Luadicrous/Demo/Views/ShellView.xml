<?xml version="1.0" encoding="UTF-8" ?>
<Control BindingContext="ViewModels/ShellViewModel.lua">
	<VerticalPanel>
		<Text Text="{Binding Text}" />
		<HorizontalPanel>
			<Button Click="{Binding AcceptText}" >
				<Label Text="Ok" />
			</Button>
			<Button Click="{Binding ClearText}" >
				<Label Text="Not Ok" />
			</Button>
		</HorizontalPanel>
	</VerticalPanel>
</Control>